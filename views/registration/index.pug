    form.row
      .col-md-3
      .col-md-6.info
        h3 Đăng ký lớp học
        div
          label Họ và tên
          input.form-control(type='text' placeholder='Họ và tên')
        div
          label Khóa học
          input.form-control(type='text' placeholder='Enter email')
          small We'll never share your email with anyone else.
        div
          br
          label Subject
          input.form-control(type='email' placeholder='Subject')
        div
          label Message
          textarea#exampleFormControlTextarea1.form-control(rows='3')
          br
        button.btn.btn-primary(type='submit') Submit
