body
  // NAVBAR
  nav.navbar.navbar-expand-lg.navbar-light.bg-light.head
      a.navbar-brand.GD TRUNG TÂM ANH NGỮ
      #navbarSupportedContent.collapse.navbar-collapse.other
        ul.navbar-nav.mr-auto
          li.nav-item
            a.nav-link(href='/home') Trang chủ
          li.nav-item
            a.nav-link(href='/course') Khóa học
          li.nav-item.dropdown
            a#navbarDropdown.nav-link.dropdown-toggle(href='#' role='button' data-bs-toggle='dropdown' aria-expanded='false')
              | Tài khoản
            ul.dropdown-menu(aria-labelledby='navbarDropdown')
              li
                a.dropdown-item(href='#') Thông tin cá nhân
              li
                a.dropdown-item(href='/logout') Đăng xuất
          li.nav-item
            a.nav-link(href='#') Hỗ trợ
          li.nav-item
            a.nav-link(href='#') Liên hệ
          
  // Home
  .container
          // Khóa học
          .small-container
            h2.title Các khóa học
            .row1
                each course in courseList
                  .col-4
                    img(src='dist/img/ielts-4.5.1.png')
                    h2=course.coursename
                    .rating
                      i.fa.fa-star
                      i.fa.fa-star
                      i.fa.fa-star
                      i.fa.fa-star
                      i.fas.fa-star-half-alt
                    p Mức độ #{course.courselevel}
                    p Đầu vào: #{course.input}
                    p Đầu ra: #{course.output}
                    p Hình thức: #{course.formulation}
                    p Học phí: #{course.fee} VND
                    p Số giờ học: #{course.totaltime} giờ 
                    // Modal
                    button.btn.btn-primary(type='button' data-toggle='modal' data-target='.classModal' ) Chi tiết
                  .modal.fade.classModal(tabindex='-1' role='dialog' aria-labelledby='myLargeModalLabel' aria-hidden='true')
                    .modal-dialog.modal-lg
                      .modal-content
                        table#tab-faculty.table.table-bordered.table-striped
                          thead
                            tr
                              th Mã số lớp
                              th Ngày bắt đầu
                              th Ngày kết thúc
                              th Lịch học
                              th Chi nhánh
                              th
                          tbody
                            each faculty in classList
                              tr 
                                td=faculty.ID
                                td=moment(faculty.startday).format('DD/MM/YYYY')
                                td=moment(faculty.endday).format('DD/MM/YYYY')
                                td=faculty.schedule
                                td=faculty.branch.address
                                td.text-center
                                  a.btn.btn-primary(style="margin-right: 5px;", href='/registration/'+ faculty.ID) Chi tiết
                                  
                                  
                        .modal-footer
                          button.btn.btn-secondary(type='button' data-dismiss='modal') Đóng
                                            
                      
                          

